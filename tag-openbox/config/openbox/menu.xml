			</item>
			<separator/>
			<item label="Exit">
				<action name="Execute">
					<prompt>
            Are you sure?
					</prompt>
					<execute>
						openbox --exit
					</execute>
				</action>
			</item>
			<item label="Shutdown">
				<action name="Execute">
					<prompt>
          Are you sure?
					</prompt>
					<execute>
          systemctl poweroff
					</execute>
				</action>
			</item>
			<item label="Restart">
				<action name="Execute">
					<prompt>
          Are you sure?
					</prompt>
					<execute>
          systemctl reboot
					</execute>
				</action>
			</item>
			<item label="Suspend">
				<action name="Execute">
					<prompt>
          Are you sure?
					</prompt>
					<execute>
          systemctl suspend
					</execute>
				</action>
			</item>
			<item label="Hibernate">
				<action name="Execute">
