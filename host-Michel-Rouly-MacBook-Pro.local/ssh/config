ForwardAgent yes

Host jump
   Hostname jump.audax.in
   DynamicForward 3128
   LocalForward 3199 partner-proxy.infra.audax.in:3128
   User     mrouly
   ServerAliveInterval 60
   IdentityFile ~/.ssh/id_rsa

Host old+*
    ProxyCommand nc -x localhost:3128 $(echo %h | sed 's/^.*+//;') %p

